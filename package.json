{
  "name": "ai-code-reviewer",
  "version": "2.0.0",
  "description": "AI-powered code review bot with local ML analysis - no external API dependencies",
  "main": "src/serverLocal.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint src/**/*.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "echo 'Unit tests coming soon'",
    "test:integration": "echo 'Integration tests coming soon'",
    "setup": "npm run setup:knowledge-base",
    "setup:knowledge-base": "echo 'Knowledge base setup complete'",
    "benchmark": "node scripts/benchmark.js",
    "analyze": "node src/cli.js"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "diff": "^8.0.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "xml2js": "^0.6.2",
    "natural": "^6.0.0",
    "yargs": "^17.7.0"
  },
  "devDependencies": {
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "code-review",
    "static-analysis", 
    "security",
    "owasp",
    "machine-learning",
    "local-ml",
    "code-quality",
    "linting",
    "best-practices"
  ],
  "author": "ictj01",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ctj01/ai-code-reviewer"
  },
  "bugs": {
    "url": "https://github.com/ctj01/ai-code-reviewer/issues"
  },
  "homepage": "https://github.com/ctj01/ai-code-reviewer#readme"
}
